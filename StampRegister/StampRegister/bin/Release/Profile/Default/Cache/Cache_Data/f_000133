./api/sticker';\n\nconst STORE_API_DETAIL = 'api:detail';\nconst STORE_API_IMAGE = 'api:image';\nconst STORE_API_PRICE = 'api:price';\n\nexport const storeStickerDetail = {\n    namespaced: true,\n    modules: {\n        [STORE_API_DETAIL]: storeStickerApiDetail,\n        [STORE_API_IMAGE]: storeStickerApiListImages,\n        [STORE_API_PRICE]: createStoreSharedPrice(StickerModel),\n    },\n    state() {\n        return {\n            stickerId: '',\n        };\n    },\n    getters: {\n        sticker({ stickerId }, getters) {\n            return getters[`${STORE_API_DETAIL}/get`](stickerId);\n        },\n        images({ stickerId }, getters) {\n            return getters[`${STORE_API_IMAGE}/get`](stickerId);\n        },\n        priceTiers(state) {\n            return state[STORE_API_PRICE].list;\n        },\n        updateRequest(state, getters) {\n            if (!getters.sticker || !getters.sticker.updateRequest) {\n                return null;\n            }\n            const result = Object.assign({}, getters.sticker);\n            Object.assign(result, getters.sticker.updateRequest);\n            // In sticker we don't have updateRequestId exposed from server,\n            // but this is being used to determine whether it's a update request\n            // or not. So we just assign some non-zero value as its id.\n            result.updateRequestId = 1;\n            return result;\n        },\n        tier(state, getters) {\n            const { sticker, priceTiers } = getters;\n            return priceTiers.find(tier => tier.tier === sticker.tierId);\n        },\n        pendingTier(state, getters) {\n            const { sticker, priceTiers } = getters;\n            return priceTiers.find(tier => tier.tier === sticker.pendingTierId);\n        },\n        hasAllMandatoryImages({ stickerId }, getters) {\n            return getters[`${STORE_API_IMAGE}/hasAllMandatoryImages`](\n                stickerId,\n            );\n        },\n        reviewableState(state, getters) {\n            const { sticker } = getters;\n            return new StateIndicator(\n                Object.assign({}, sticker),\n                'sticker',\n                sticker.campaign,\n            );\n        },\n        reviewableStateOfUpdateRequest(state, getters) {\n            const { sticker, updateRequest } = getters;\n            return new StateIndicator(\n                updateRequest,\n                'sticker',\n                sticker.campaign,\n            );\n        },\n        isStaticType(state, getters) {\n            return getters.sticker.stickerType === 'static';\n        },\n        isAnimatedType(state, getters) {\n            return getters.sticker.stickerType === 'animation';\n        },\n        isCustomType(state, getters) {\n            return getters.sticker.stickerType === 'custom';\n        },\n        isMessageType(state, getters) {\n            return getters.sticker.stickerType === 'message';\n        },\n        isBigType(state, getters) {\n            return getters.sticker.stickerType === 'big';\n        },\n        isPopupType(state, getters) {\n            return getters.sticker.stickerType === 'popup';\n        },\n        isEffectType(state, getters) {\n            return getters.sticker.stickerType === 'effect';\n        },\n        popupAppendix(state, getters) {\n            return (\n                (getters.sticker.stickerType === 'popup' && '_popup') ||\n                (getters.sticker.stickerType === 'effect' && '_effect') ||\n                ''\n            );\n        },\n        isUpdateRequest(state, getters) {\n            return !!getters.updateRequest;\n        },\n        isReleased(state, getters) {\n            return getters.sticker.releasedOn > 0;\n        },\n    },\n    actions: {\n        fetch({ commit, dispatch }, id) {\n            commit('setStickerId', id);\n            return Promise.all([\n                dispatch(`${STORE_API_DETAIL}/fetch`, id),\n                dispatch(`${STORE_API_IMAGE}/fetch`, id),\n            ]).then(() => {\n                dispatch('initiatePrices');\n            });\n        },\n        initiatePrices({ getters, commit }) {\n            const { isReleased, tier, pendingTier } = getters;\n            commit(\n                `${STORE_API_PRICE}/setCurrentPrice`,\n                isReleased ? tier : pendingTier || tier,\n            );\n            commit(`${STORE_API_PRICE}/setSelectedPrice`, pendingTier || tier);\n        },\n        saveSelectedPriceTier({ state, getters, dispatch }) {\n            const { selectedPrice } = state[STORE_API_PRICE];\n            return StickerModel.savePriceTier(\n                getters.sticker.id,\n                selectedPrice.tier,\n            ).then(({ secondsToRelease }) => {\n                return dispatch('setSecondsToRelease', secondsToRelease);\n            });\n        },\n        clearSecondsToRelease(context) {\n            context.commit(\n                `${STORE_API_DETAIL}/clearSecondsToRelease`,\n                context.state.stickerId,\n            );\n        },\n        setSecondsToRelease(context, secondsToRelease) {\n            return context.commit(`${STORE_API_DETAIL}/setSecondsToRelease`, {\n                id: context.state.stickerId,\n                secondsToRelease,\n            });\n        },\n    },\n    mutations: {\n        setStickerId(state, id) {\n            state.stickerId = id;\n        },\n    },\n};\n","import { storeStickerApiDetail } from './api/detail';\nimport { storeStickerApiListImages } from './api/list_images';\nimport doFetch from '../../../models/do-fetch';\nimport StateIndicator from '../../../models/state-indicator';\nimport { makeSequence } from '../../../utils/helpers';\n\nexport const STICKERS_PER_SET = {\n    '8_TO_40': [8, 16, 24, 32, 40],\n    '8_TO_24': [8, 16, 24],\n};\n\nexport const resolveImage = (key, imageMap, errorMap, positionMap) => {\n    const url = imageMap[key];\n    const errors = errorMap[key];\n    if (/\\d\\d_(popup|effect)$/.test(key)) {\n        return {\n            key,\n            url,\n            errors,\n            position: positionMap[key] || 'center',\n        };\n    }\n    return { key, url, errors };\n};\n\nexport const storeStickerImage = {\n    namespaced: true,\n    modules: {\n        'api:detail': storeStickerApiDetail,\n        'api:list_images': storeStickerApiListImages,\n    },\n    state: {\n        stickerId: null,\n        stickersPerSet: 0,\n        positionMap: {},\n        zipErrors: null,\n        imageError: {},\n        hasChanged: false,\n    },\n    getters: {\n        sticker(state, getters) {\n            return getters['api:detail/get'](state.stickerId);\n        },\n        stickerType(state, getters) {\n            return getters.sticker.stickerType;\n        },\n        reviewableState(state, getters) {\n            const { sticker } = getters;\n            return new StateIndicator(\n                Object.assign({}, sticker),\n                'sticker',\n                sticker.campaign,\n            );\n        },\n        imageMap(state, getters) {\n            return getters['api:list_images/get'](state.stickerId).images;\n        },\n        /**\n         * @returns The list of all required image keys (not existing image keys)\n         */\n        imageKeys(state, getters) {\n            const { stickerType, numberOfImages, isPopupType, isEffectType } =\n                getters;\n            if (!numberOfImages) {\n                return [];\n            }\n            const keys = ['main', 'tab', ...makeSequence(numberOfImages)];\n            if (isPopupType || isEffectType) {\n                return keys.flatMap(key => {\n                    return key === 'tab' ? key : [key, `${key}_${stickerType}`];\n                });\n            }\n            return keys;\n        },\n        imagePairs(state, getters) {\n            // For Popup/Effect stickers\n            const { stickerType, imageMap, numberOfImages } = getters;\n            if (!imageMap) {\n                return [];\n            }\n            const { imageError, positionMap } = state;\n            const keys = ['main', 'tab', ...makeSequence(numberOfImages)];\n            return keys.map(key => {\n                const preview = resolveImage(key, imageMap, imageError);\n                if (key === 'tab') {\n                    return { key, preview };\n                }\n                const popup = resolveImage(\n                    `${key}_${stickerType}`,\n                    imageMap,\n                    imageError,\n                    positionMap,\n                );\n                return { key, preview, popup };\n            });\n        },\n        numberOfImages(state, getters) {\n            return getters['api:list_images/get'](state.stickerId)\n                .numberOfImages;\n        },\n        stickersPerSetOptions(state, getters) {\n            const { isStaticType, isCustomType, isBigType } = getters;\n            if (isStaticType || isCustomType || isBigType) {\n                return STICKERS_PER_SET['8_TO_40'];\n            }\n            return STICKERS_PER_SET['8_TO_24'];\n        },\n        isUpdateRequest(state, getters) {\n            const { sticker } = getters;\n            return sticker && !!sticker.updateRequest;\n        },\n        isStaticType(state, getters) {\n            return getters.stickerType === 'static';\n        },\n        isAnimatedType(state, getters) {\n            return getters.stickerType === 'animation';\n        },\n        isCustomType(state, getters) {\n            return getters.stickerType === 'custom';\n        },\n        isMessageType(state, getters) {\n            return getters.stickerType === 'message';\n        },\n        isBigType(state, getters) {\n            return getters.stickerType === 'big';\n        },\n        isPopupType(state, getters) {\n            return getters.stickerType === 'popup';\n        },\n        isEffectType(state, getters) {\n            return getters.stickerType === 'effect';\n        },\n        isImagesEditable(state, getters) {\n            const { sticker, reviewableState, isUpdateRequest } = getters;\n            if (isUpdateRequest || sticker.isTooOldToUpload) {\n                return false;\n            }\n            return reviewableState.canEdit();\n        },\n        hasAllMandatoryImages(state, getters) {\n            return (\n                Object.keys(getters.imageMap).length ===\n                getters.imageKeys.length\n            );\n        },\n        hasImageError(state) {\n            return Object.values(state.imageError).some(\n                errors => errors && errors.length > 0,\n            );\n        },\n    },\n    actions: {\n        fetch(context, id) {\n            context.commit('setStickerId', id);\n            return Promise.all([\n                context.dispatch('api:detail/fetch', id),\n                context.dispatch('api:list_images/fetch', id),\n            ]).then(() => {\n                context.commit(\n                    'setStickersPerSet',\n                    context.getters.numberOfImages,\n                );\n                const { isPopupType, isEffectType } = context.getters;\n                if (isPopupType || isEffectType) {\n                    return context.dispatch('fetchPosition', id);\n                }\n            });\n        },\n        upload(context, { id, key, file }) {\n            const { stickerType } = context.getters.sticker;\n            return context\n                .dispatch('api:list_images/upload', {\n                    id,\n                    stickerType,\n                    key,\n                    file,\n                })\n                .then(() => {\n                    context.commit('clearImageError', key);\n                })\n                .catch(error => {\n                    if (error.detail) {\n                        context.commit('setImageError', {\n                            [key]: error.detail.errors,\n                        });\n                    } else {\n                        console.error(error);\n                    }\n                });\n        },\n        fetchPosition(context, id) {\n            return doFetch('GET', `api/sticker/${id}/popup-sticker/position`, {\n                camelizeIgnoreAll: true,\n            }).then(data => {\n                context.commit('setPosition', data);\n            });\n        },\n        updatePosition(context, id) {\n            const { positionMap } = context.state;\n            return doFetch('PUT', `api/sticker/${id}/popup-sticker/position`, {\n                data: positionMap,\n            });\n        },\n        updateStickersPerSet(context, id) {\n            const { stickersPerSet } = context.state;\n            return context.dispatch('api:list_images/updateStickersPerSet', {\n                id,\n                stickersPerSet,\n            });\n        },\n    },\n    mutations: {\n        setStickerId(state, id) {\n            state.stickerId = id;\n        },\n        setStickersPerSet(state, stickersPerSet) {\n            state.stickersPerSet = stickersPerSet;\n        },\n        setPosition(state, map) {\n            state.positionMap = { ...map };\n        },\n        setPositionOf(state, { key, position }) {\n            state.positionMap = { ...state.positionMap, [key]: position };\n        },\n        insertPosition(state, keys) {\n            const { positionMap } = state;\n            keys.forEach(key => {\n                positionMap[key] = 'center';\n            });\n            state.positionMap = { ...positionMap };\n        },\n        removePosition(state, keys) {\n            const { positionMap } = state;\n            keys.forEach(key => {\n                delete positionMap[key];\n            });\n            state.positionMap = { ...positionMap };\n        },\n        setZipErrors(state, errors) {\n            state.zipErrors = [...errors];\n        },\n        clearZipErrors(state) {\n            state.zipErrors = [];\n        },\n        setImageError(state, errors) {\n            state.imageError = {\n                ...state.imageError,\n                ...errors,\n            };\n        },\n        clearImageError(state, key) {\n            if (key) {\n                state.imageError = { ...state.imageError, [key]: [] };\n            } else {\n                state.imageError = {};\n            }\n        },\n        setHasChanged(state, value) {\n            state.hasChanged = value;\n        },\n    },\n};\n","<script>\nimport camelize from 'camelize';\n\nimport AreaChecklist from '../product/area-checklist.vue';\nimport FormError from '../product/form-error.vue';\nimport {\n    userStickerCMSSellerPath,\n    hasPremiumLaunchedAreas,\n    timestampToDate,\n    timestampToDateTime,\n} from '../../utils/helpers';\nimport { metaMapToList, areaListToMap } from '../../utils/component-helpers';\nimport doFetch from '../../models/do-fetch';\nimport ConfirmDeleteSticker from '../dialog/confirm-delete-sticker.vue';\nimport ConfirmRemovingUpdateRequest from '../dialog/confirm-removing-update-request.vue';\n\nexport default {\n    components: {\n        AreaChecklist,\n        FormError,\n    },\n    inject: ['LANGUAGE'],\n    inheritAttrs: false,\n    props: {\n        isInfoOnStore: {\n            type: Boolean,\n            required: true,\n        },\n        sticker: {\n            type: Object,\n            required: true,\n        },\n        stickerState: {\n            type: Object,\n            required: true,\n        },\n        author: {\n            type: Object,\n            required: true,\n        },\n        seller: {\n            type: Object,\n            required: true,\n        },\n        categoryMap: {\n            type: Object,\n            required: true,\n        },\n        languages: {\n            type: Array,\n            required: true,\n        },\n        ca