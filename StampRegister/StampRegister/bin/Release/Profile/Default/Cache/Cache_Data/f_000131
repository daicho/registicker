\nexport function resolveContentType({ data, config }) {\n    const { contentType } = config;\n    const isFormDataContent = contentType === 'multipart/form-data';\n    const { hasBlobValue, props } = checkBlobValue(data);\n    if (isFormDataContent || hasBlobValue) {\n        return { contentType: 'multipart/form-data', blobProps: props };\n    }\n\n    if (data) {\n        return { contentType: 'application/json' };\n    }\n\n    // when the request body is empty, don't return the content type\n    return {};\n}\n\nexport function resolveHeaders({ method, contentType, config }) {\n    const isJsonContent = contentType === 'application/json';\n    const prefetchable = method === 'GET' && !isAndroid;\n    const headerItems = prefetchable\n        ? {}\n        : {\n              'X-Requested-With': 'XMLHttpRequest',\n          };\n    const headers = new window.Headers(headerItems);\n    if (/^(post|put|delete)$/i.test(method)) {\n        headers.append('X-XSRF-TOKEN', userStickerCMSXsrfToken());\n    }\n    if (isJsonContent && !prefetchable) {\n        headers.append('Content-Type', 'application/json');\n    }\n    if (config.headers) {\n        Object.keys(config.headers).forEach(key => {\n            headers.append(key, config.headers[key]);\n        });\n    }\n    // browser would decide content-type header for blob itself\n    return headers;\n}\n\nexport function resolveRequestBody({ data, dataAsIs, contentType, blobProps }) {\n    const isJsonContent = contentType === 'application/json';\n    if (!data) {\n        return null;\n    }\n    if (isJsonContent) {\n        return JSON.stringify(\n            Object.assign(decamelizeKeysDeep(data), dataAsIs),\n        );\n    }\n    const formData = new FormData();\n    const jsonToFormDataOptions = {\n        initialFormData: formData,\n        showLeafArrayIndexes: false,\n        includeNullValues: false,\n    };\n    if (blobProps && blobProps.length) {\n        const jsonProps = Object.keys(data).filter(\n            key => !blobProps.includes(key),\n        );\n        const jsonData = pick(data, jsonProps);\n        jsonToFormData(decamelizeKeysDeep(jsonData), jsonToFormDataOptions);\n        blobProps.forEach(key => {\n            const name = decamelizeKeysDeep(key);\n            const value = data[key];\n            if (Array.isArray(value)) {\n                value.forEach(blob => formData.append(`${name}[]`, blob));\n            } else {\n                formData.append(name, value);\n            }\n        });\n    } else {\n        jsonToFormData(decamelizeKeysDeep(data), jsonToFormDataOptions);\n    }\n    if (dataAsIs) {\n        jsonToFormData(dataAsIs, jsonToFormDataOptions);\n    }\n    return formData;\n}\n\nexport function isBlob(val) {\n    return val instanceof Blob;\n}\n\n// for shallow check only\nexport function checkBlobValue(data) {\n    if (!data) {\n        return {\n            hasBlobValue: false,\n            props: [],\n        };\n    }\n    const props = Object.keys(data)\n        .map(prop => {\n            const value = data[prop];\n            if (isBlob(value)) {\n                return prop;\n            }\n            if (Array.isArray(value) && value.length && value.every(isBlob)) {\n                return prop;\n            }\n            return null;\n        })\n        .filter(v => !!v);\n    const hasBlobValue = props.length > 0;\n    return { hasBlobValue, props };\n}\n\nexport function camelizeData(data, ignore) {\n    try {\n        if (Array.isArray(data)) {\n            return data.map(item => {\n                const camelizeProps = Object.keys(item).filter(\n                    key => !ignore.includes(key),\n                );\n                return Object.assign(\n                    camelize(pick(item, camelizeProps)),\n                    pick(item, ignore),\n                );\n            });\n        }\n        const camelizeProps = Object.keys(data).filter(\n            key => !ignore.includes(key),\n        );\n        return Object.assign(\n            camelize(pick(data, camelizeProps)),\n            pick(data, ignore),\n        );\n    } catch (err) {\n        return null;\n    }\n}\n\nexport function convertResponseText(text, config) {\n    if (!text) {\n        return;\n    }\n    // because angular requests to prepend )]}',\n    // to the response of ajax request\n    // so we have to manually remove it\n    if (/^\\)/.test(text)) {\n        text = text.slice(6);\n    }\n    let data;\n    try {\n        data = JSON.parse(text);\n    } catch (error) {\n        return text;\n    }\n    const { camelizeIgnoreAll, camelizeIgnore = [] } = config;\n    return camelizeIgnoreAll ? data : camelizeData(data, camelizeIgnore);\n}\n\n// TODO: some feature can be implemented here\n// 1. should we camelize response here\n// 2. do we need a cache system\n\n/**\n * @typedef Config\n * @type {Object}\n * @property {Object} [params] would be transform to queryString and append to get url\n * @property {Object} [data] content of body\n * @property {Object} [headers] request headers\n * @property {String} [contentType] content type of body\n * @property {Array} [camelizeIgnore] a list containing keys that should not be camelized\n * @property {String} [observe] observe target. Available value in ['response', 'body']\n */\n\n/**\n * @typedef FetchError\n * @type {Object}\n * @property {Int} status code\n * @property {String} text error message\n */\n\n/**\n * return a promise fetch given request settings\n * @template T\n * @return {Promise<T>}\n * @param {String} method http method\n * @param {String} originalUrl url to request\n * @param {Config} config a config instance\n * @throws {Error} an Error and it's messsage is a JSON.stringified {@link FetchError}\n */\nexport function doFetch(method, originalUrl, config = {}) {\n    const { data, dataAsIs, params, observe = 'body' } = config;\n    const url = resolveUrl({ url: originalUrl, params });\n    const { contentType, blobProps } = resolveContentType({ data, config });\n    const headers = resolveHeaders({ method, contentType, config });\n    const body = resolveRequestBody({ data, dataAsIs, contentType, blobProps });\n    const isObservingResponse = observe === 'response';\n    const request = new window.Request(url, {\n        method,\n        headers,\n        // XXX:\n        //   Before Chrome v68, the default credentials will be 'omit'\n        credentials: 'same-origin',\n        body,\n    });\n    return window.fetch(request).then(response => {\n        const { ok, status } = response;\n        if (!ok) {\n            return response.text().then(text => {\n                throw new Error(JSON.stringify({ status, text }));\n            });\n        }\n        if (isObservingResponse) {\n            return response;\n        }\n        return response.text().then(text => convertResponseText(text, config));\n    });\n}\n\nexport default doFetch;\n","import invert from 'lodash/invert';\nimport memoize from 'lodash/memoize';\n\nimport doFetch from './do-fetch';\nimport camelize from 'camelize';\n\nconst fetchLanguages = memoize(() => {\n    return doFetch('GET', 'api/sticker/supported_languages').then(\n        data => data.languages,\n    );\n});\n\nexport class Languages {\n    /**\n     * @typedef Language\n     * @property {String} code\n     * @property {String} name\n     *\n     * @returns {Promise.<Language[]>}\n     */\n    static fetch() {\n        return fetchLanguages();\n    }\n\n    static findLanguageByCode(code, languages) {\n        if (!languages) {\n            throw new Error(`Parameter 'languages' must be given`);\n        }\n        return languages.find(\n            language => camelize(language.code) === camelize(code),\n        );\n    }\n\n    /**\n     * @param {Language[]} languages\n     */\n    constructor(languages) {\n        this.list = languages;\n        languages.forEach(lang => (this[lang.code] = lang));\n    }\n\n    makeSortComparator() {\n        const position = invert(this.list.map(v => v.code));\n        return (a, b) => position[a.code] - position[b.code];\n    }\n}\n","export class ReviewCounter {\n    constructor(count, limit) {\n        this.count = count;\n        this.limit = limit;\n    }\n\n    get isCountZero() {\n        return this.count === 0;\n    }\n\n    get isBelowLimit() {\n        return this.count < this.limit;\n    }\n}\n","import { isOnlySoldInId } from '../utils/component-helpers';\nconst EDITING = 'editing';\nconst WAITING_FOR_REVIEW = 'waiting_for_review';\nconst UPLOADING = 'uploading';\nconst IN_REVIEW = 'in_review';\nconst IN_REVIEW_1 = 'in_review_1';\nconst IN_REVIEW_2 = 'in_review_2';\nconst IN_REVIEW_3 = 'in_review_3';\nconst WAITING_FOR_BILLING = 'waiting_for_billing';\nconst WAITING_FOR_APPROVE = 'waiting_for_approve';\nconst IN_REVIEW_UNDER_PROCESSING = 'in_review_under_processing';\nconst REJECTED = 'rejected';\nconst READY_FOR_SALE = 'ready_for_sale';\nconst APPROVED = 'approved';\nconst STOP_SALE = 'stop_sale';\nconst BANNED = 'banned';\nconst MINIMUM_STICKERS_PER_SET = 8;\n\nexport const STATE_PROPS = {\n    emoji: 'confirmationStatus',\n    sticker: 'confirmationStatusForSeller',\n    theme: 'confirmationStatusForSeller',\n};\n\nconst CAN_RELEASE_PROPS = {\n    sticker: 'canRelease',\n    emoji: 'canReleaseToShop',\n    theme: 'canRelease',\n};\n\nconst DUPLICATED_TITLE_PROPS = {\n    sticker: 'duplicatedTitleError',\n    emoji: 'hasDuplicatedTitle',\n    theme: 'duplicatedTitleError',\n};\n\nconst AREAS_PROPS = {\n    sticker: 'areas',\n    emoji: 'areaCodes',\n    theme: 'areas',\n};\n\nconst TIER_PROPS = {\n    sticker: 'tierId',\n    emoji: 'tier',\n    theme: 'tierId',\n};\n\nexport class StateIndicator {\n    constructor(product, type, campaign) {\n        this.product = product;\n        this.campaign = campaign;\n        this.type = type;\n        this.stateProp = STATE_PROPS[type];\n        this.canReleaseProp = CAN_RELEASE_PROPS[type];\n        this.duplicatedTitleProp = DUPLICATED_TITLE_PROPS[type];\n        this.areasProp = AREAS_PROPS[type];\n        this.tierProp = TIER_PROPS[type];\n    }\n\n    get isEmoji() {\n        return this.type === 'emoji';\n    }\n\n    get isTheme() {\n        return this.type === 'theme';\n    }\n\n    get isSticker() {\n        return this.type === 'sticker';\n    }\n\n    get isStaticSticker() {\n        return this.isSticker && this.product.stickerType === 'static';\n    }\n\n    get isAnimationSticker() {\n        return this.isSticker && this.product.stickerType === 'animation';\n    }\n\n    get isMinimumStickersPerSet() {\n        return (\n            parseInt(this.product.stickersPerSet, 10) ===\n            MINIMUM_STICKERS_PER_SET\n        );\n    }\n\n    get isUpdateRequest() {\n        return !!this.product.isUpdateRequest || !!this.product.updateRequestId;\n    }\n\n    get timeFromWhichReopenIsAllowed() {\n        if (\n            !Object.prototype.hasOwnProperty.call(\n                this.product,\n                'secondsToRelease',\n            )\n        ) {\n            console.warn(\n                'secondsToRelease does not exist in data. There may be errors upon querying emoji detail.',\n            );\n            return Date.now();\n        }\n        return new Date(\n            this.product.timestamp + 1000 * this.product.secondsToRelease,\n        );\n    }\n\n    get timeFromWhichReopenWithTierChangeIsAllowed() {\n        if (\n            !Object.prototype.hasOwnProperty.call(\n                this.product,\n                'secondsToReleaseTierChange',\n            )\n        ) {\n            console.warn(\n                'secondsToReleaseTierChange does not exist in data. There may be errors upon querying emoji detail.',\n            );\n            return Date.now();\n        }\n        return new Date(\n            this.product.timestamp +\n                1000 * this.product.secondsToReleaseTierChange,\n        );\n    }\n    get timeFromWhichTierChangeIsAllowed() {\n        if (\n            !Object.prototype.hasOwnProperty.call(\n                this.product,\n                'secondsToNextTierChangeAllowed',\n            )\n        ) {\n            console.warn(\n                'secondsToNextTierChangeAllowed does not exist in data. There may be errors upon querying emoji detail.',\n            );\n            return Date.now();\n        }\n        return new Date(\n            this.product.timestamp +\n                1000 * this.product.secondsToNextTierChangeAllowed,\n        );\n    }\n    get canUpdateCampaign() {\n        return !this.isUpdateRequest && this.isEditingOrRejected();\n    }\n\n    get canReleaseToShop() {\n        return this.product[this.canReleaseProp];\n    }\n\n    get hasDuplicatedTitle() {\n        return this.product[this.duplicatedTitleProp];\n    }\n\n    get areas() {\n        return this.product[this.areasProp];\n    }\n\n    get tier() {\n        return parseInt(this.product[this.tierProp], 10);\n    }\n\n    get state() {\n        return this.product[this.stateProp];\n    }\n\n    // As simple as not to be wrapped\n    isEditing() {\n        return this.state === EDITING;\n    }\n    // As simple as not to be wrapped\n    isWaitingForReview() {\n        return this.state === WAITING_FOR_REVIEW || this.state === UPLOADING;\n    }\n\n    isInReview() {\n        return (\n            this.state === IN_REVIEW ||\n            this.state === IN_REVIEW_1 ||\n            this.state === IN_REVIEW_2 ||\n            this.state === IN_REVIEW_3 ||\n            this.state === WAITING_FOR_BILLING ||\n            this.state === WAITING_FOR_APPROVE ||\n            this.state === IN_REVIEW_UNDER_PROCESSING\n        );\n    }\n\n    isApproved() {\n        return this.state === APPROVED;\n    }\n\n    isReadyForSale() {\n        return this.state === READY_FOR_SALE;\n    }\n\n    isSaleStopped() {\n        return this.state === STOP_SALE;\n    }\n    // As simple as not to be wrapped\n    isBanned() {\n        return this.state === BANNED;\n    }\n    // As simple as not to be wrapped\n    isRejected() {\n        return this.state === REJECTED;\n    }\n\n    isEditingOrRejected() {\n        return this.isRejected() || this.isEditing();\n    }\n\n    isExpiredSalePeriod() {\n        const collaborationExpired =\n            this.product.collaboration &&\n            !this.product.collaboration?.isSellable;\n        const campaignExpired =\n            this.campaign &&\n            this.campaign.endsSaleOn &&\n            this.campaign.endsSaleOn < new Date().getTime() / 1000;\n        return collaborationExpired || campaignExpired;\n    }\n\n    isOutOfSalePeriod() {\n        const collaborationOutOfPeriod =\n            this.product.collaboration &&\n            !this.product.collaboration.isSellable;\n        const campaignOutOfPeriod =\n            this.isWaitingSalePeriod() || this.isExpiredSalePeriod();\n        return campaignOutOfPeriod || collaborationOutOfPeriod;\n    }\n\n    // Used only in statusbar\n    isRejectedOrBanned() {\n        return this.isRejected() || this.isBanned();\n    }\n\n    isWaitingSalePeriod() {\n        return (\n            this.campaign &&\n            this.campaign.startsSaleOn &&\n            this.campaign.startsSaleOn > new Date().getTime() / 1000\n        );\n    }\n\n    isTier02() {\n        return this.tier === 10002;\n    }\n\n    isTier06() {\n        return this.tier === 10006;\n    }\n\n    isValidForTier02() {\n        return (\n            this.isMinimumStickersPerSet &&\n            !this.product.allArea &&\n            isOnlySoldInId(this.areas)\n        );\n    }\n\n    isValidForTier06() {\n        return (\n            this.isSticker &&\n            this.isMinimumStickersPerSet &&\n            !this.product.allArea &&\n            isOnlySoldInId(this.areas)\n        );\n    }\n\n    isValidForTier1(isIndonesian) {\n        if (\n            this.isEmoji ||\n            this.isTheme ||\n            this.isStaticSticker ||\n            (this.isAnimationSticker &&\n                isIndonesian &&\n                isOnlySoldInId(this.areas))\n        ) {\n            return true;\n        }\n        return false;\n    }\n\n    canRequestApplication() {\n        if (\n            !Object.prototype.hasOwnProperty.call(\n                this.product,\n                'canRequestReview',\n            )\n        ) {\n            console.warn(\n                'canRequestReview does not exist in Emoji data. There may be errors upon querying emoji detail.',\n            );\n        }\n        return !!this.product.canRequestReview;\n    }\n\n    canCancelScheduledSale() {\n        return this.product.scheduledForSale;\n    }\n\n    canRequestStartSelling() {\n        return this.canReleaseToShop;\n    }\n\n    canScheduleSale() {\n        return this.isWaitingSalePeriod() && this.canReleaseToShop;\n    }\n\n    canStopSelling() {\n        return this.isReadyForSale();\n    }\n\n    canDelete() {\n        return this.isEditing() || this.isRejected() || this.isApproved();\n    }\n\n    canEdit() {\n        return this.isEditing() || this.isRejected();\n    }\n\n    canCreateUpdateRequest() {\n        return (\n            (this.isReadyForSale() || this.isSaleStopped()) &&\n            !this.isExpiredSalePeriod()\n        );\n    }\n\n    canEditTag() {\n        return !(\n            this.isWaitingForReview() ||\n            this.isInReview() ||\n            this.isApproved() ||\n            this.isBanned()\n        );\n    }\n\n    //canEditUpdateRequest() {\n    //    return (\n    //        (this.isApproved() ||\n    //            this.isReadyForSale() ||\n    //            this.isSaleStopped()) &&\n    //        (!this.updateRequest ||\n    //            this.updateRequest.isEditing() ||\n    //            this.updateRequest.isRejected())\n    //    );\n    //}\n\n    canChangePriceTier() {\n        if (this.isReadyForSale() || this.isBanned()) {\n            return false;\n        } else if (this.isSaleStopped()) {\n            return this.secondsBeforeTierChangeIsAllowed() === 0;\n        } else {\n            return true;\n        }\n    }\n    canChangeUsePhoto() {\n        if (\n            this.isUpdateRequest ||\n            this.isApproved() ||\n            this.isReadyForSale() ||\n            this.isSaleStopped() ||\n            this.isBanned()\n        ) {\n            return false;\n        }\n        return true;\n    }\n    canCancelRequest() {\n        if (this.product.updateRequestId) {\n            return (\n                this.isWaitingForReview() ||\n                this.isInReview() ||\n                this.isApproved()\n            );\n        }\n        if (\n            !Object.prototype.hasOwnProperty.call(\n                this.product,\n                'canCancelRequest',\n            )\n        ) {\n            console.warn(\n                'canCancelRequest does not exist in data. There may be errors upon querying emoji detail.',\n            );\n        }\n        return (\n            !!this.product.canCancelRequest && !this.product.scheduledForSale\n        );\n    }\n\n    canDisplayFeedbackMessageOfAutoSuggest() {\n        return !this.isBanned();\n    }\n\n    canRequestReselling() {\n        return !!(\n            this.canReleaseToShop &&\n            this.secondsBeforeReopenIsAllowed() === 0 &&\n            !this.isOutOfSalePeriod()\n        );\n    }\n\n    hasBeenPublishedOnce() {\n        return (\n            this.isApproved() ||\n            this.isReadyForSale() ||\n            this.isSaleStopped() ||\n            this.isBanned()\n        );\n    }\n\n    hasBeenSoldOnce() {\n        return this.isReadyForSale() || this.isSaleStopped() || this.isBanned();\n    }\n\n    secondsBeforeReopenWithTierChangeIsAllowed() {\n        return secondsUntil(this.timeFromWhichReopenWithTierChangeIsAllowed);\n    }\n\n    secondsBeforeTierChangeIsAllowed() {\n        return secondsUntil(this.timeFromWhichTierChangeIsAllowed);\n    }\n\n    secondsBeforeReopenIsAllowed() {\n        return secondsUntil(this.timeFromWhichReopenIsAllowed);\n    }\n\n    shouldShowScheduleSellingBtn() {\n        return (\n            this.canReleaseToShop &&\n            this.isWaitingSalePeriod() &&\n            !this.isExpiredSalePeriod() &&\n            !this.product.scheduledForSale &&\n            !this.isReadyForSale()\n        );\n    }\n\n    shouldShowStartSellingBtn() {\n        return this.isApproved() && !this.isOutOfSalePeriod();\n    }\n}\n\nfunction secondsUntil(time) {\n    return Math.floor(Math.max(0, (time || 0) - Date.now()) / 1000);\n}\n\n// For compatibility. Use named export.\nexport default StateIndicator;\n","let components;\n\nif (process.env.NODE_ENV === 'test') {\n    components = require.context(\n        '../components/base',\n        false,\n        /^\\.\\/[\\w-]+(.vue)(.js)?$/,\n    );\n} else {\n    components = require.context(\n        '../components/base',\n        false,\n        /*\n            A `.vue` is generated for each `.vue.js`.\n            To avoid importing same files twice, `.vue.js` is ignored by regex.\n        */\n        /^\\.\\/[\\w-]+\\.(vue|js)(?!\\.vue\\.js)$/,\n    );\n}\n\nconst modules = components.keys().map(filename => {\n    const name = filename.replace(/^\\.\\/(.*)(.vue)(.js)?$/, '$1');\n    const module = components(filename);\n    return { name, module };\n});\n\nexport const commonComponents = {\n    install: (app, { components = {} } = {}) => {\n        const additionalModules = Object.entries(components).map(\n            ([name, module]) => ({ name, module }),\n        );\n        [...modules, ...additionalModules].map(({ name, module }) => {\n            app.component(name, module.default || module);\n        });\n    },\n};\n","/* eslint-disable no-console */\nimport * as Sentry from '@sentry/vue';\nimport { Integrations } from '@sentry/tracing';\n\nfunction isIE() {\n    if (navigator.appName == 'Microsoft Internet Explorer') {\n        return true;\n    } else if (navigator.appName == 'Netscape') {\n        const ua = navigator.userAgent;\n        const re = new RegExp('Trident/.*rv:([0-9]{1,}[\\\\.0-9]{0,})');\n        if (re.exec(ua) !== null) {\n            return true;\n        }\n    }\n    return false;\n}\n\n/**\n * normalizedPathName.\n *\n * @param {string} pathname\n * @example\n * normalizedPathName(\"/my/a1sO62d5T0a0SFyq/sticker\") // => \"/my/[token]/sticker\"\n * normalizedPathName(\"/my/a1sO62d5T0a0SFyq/sticker/112830\") // => \"/my/[token]/sticker/[id]\"\n * normalizedPathName(\"/my/a1sO62d5T0a0SFyq/sticker/112830#/image\") // => \"/my/[token]/sticker/[id]#/image\"\n * normalizedPathName(\"/my/a1sO62d5T0a0SFyq/sticker/112830/image\") // => \"/my/[token]/sticker/[id]/image\"\n */\nfunction normalizedPathName(pathname) {\n    return pathname\n        .replace(/^\\/my\\/[0-9a-zA-Z]{16}\\//, '/my/[token]/')\n        .replace(/^\\/my\\/\\[token\\]\\/(\\w+)\\/\\d+/, (_, p1) => {\n            return `/my/[token]/${p1}/[id]`;\n        });\n}\n\nexport const sentry = {\n    install: app => {\n        if (process.env.NODE_ENV === 'development') {\n            console.warn('Sentry is disabled on development env');\n            return;\n        }\n        if (\n            process.env.VUE_APP_DEPLOY_ENV !== 'beta' &&\n            process.env.VUE_APP_DEPLOY_ENV !== 'real'\n        ) {\n            console.warn('Sentry is disabled on CI and E2E');\n            return;\n        }\n        if (isIE()) {\n            console.warn('Sentry cannot work on IE');\n            return;\n        }\n        if (window.__LCM_SELLER_COUNTRY) {\n            Sentry.setTag('country_or_region', window.__LCM_SELLER_COUNTRY);\n        }\n        Sentry.setTag('is_vue3', true);\n        Sentry.init({\n            app,\n            dsn: process.env.VUE_APP_SENTRY_DNS_CMS,\n            release: process.env.VUE_APP_VERSION,\n            integrations: [\n                new Integrations.Brow