            throw new Error('PNG Fetch error for ' + this.url);\n                    }\n                    if (!response.url.includes(this.url)) {\n                        throw new Error('canceled');\n                    }\n                    const buffer = await response.arrayBuffer();\n                    const apng = parseAPNG(buffer);\n\n                    if (apng instanceof Error) {\n                        throw apng;\n                    }\n\n                    const canvas = this.$el.querySelector('canvas');\n\n                    canvas.width = apng.width;\n                    canvas.height = apng.height;\n\n                    const player = await apng.getPlayer(\n                        canvas.getContext('2d'),\n                        this.autoplay,\n                    );\n\n                    if (!response.url.includes(this.url)) {\n                        // Once the player is resolved, then it will play the APNG automatically\n                        // `anpg-js` uses `requestAnimationFrame` to play the frames\n                        // So, it can only be stopped using `requestAnimationFrame` as well\n                        requestAnimationFrame(() => player.stop());\n                        throw new Error('canceled');\n                    }\n\n                    this.player = player;\n                    this.isPlaying = this.autoplay;\n                    this.player.on('end', () => {\n                        this.$emit('end');\n                    });\n                    this.$emit('ready', this);\n                })\n                .catch(e => {\n                    if (e.message !== 'canceled') {\n                        console.error(e.message);\n                    }\n                });\n        },\n        togglePlay() {\n            if (!this.player) {\n                return;\n            }\n            if (this.isPlaying && !this.replayOnClick) {\n                this.player.pause();\n                this.isPlaying = false;\n            } else {\n                this.replay();\n            }\n        },\n        play() {\n            if (!this.player) {\n                return;\n            }\n            this.player.play();\n            this.isPlaying = true;\n        },\n        replay() {\n            if (!this.player) {\n                return;\n            }\n            this.player.stop();\n            // When playing the image right after stop it,\n            // the frame delay becomes shorter.\n            // It seems a bug in `apng-js`.\n            requestAnimationFrame(() => this.player.play());\n            this.isPlaying = true;\n        },\n        pause() {\n            if (!this.player) {\n                return;\n            }\n            this.player.pause();\n            this.isPlaying = false;\n        },\n        stop() {\n            if (!this.player) {\n                return;\n            }\n            this.player.stop();\n            this.isPlaying = false;\n        },\n        reset() {\n            this.stop();\n            this.player = null;\n            const canvas = this.$el.querySelector('canvas');\n            const context = canvas.getContext('2d');\n            context.clearRect(0, 0, canvas.width, canvas.height);\n        },\n        onImageClick() {\n            this.$emit('image-click', this);\n        },\n    },\n});\n</script>\n\n<template>\n    <section\n        :class=\"['cm-apng-player', {\n            'button-inside': buttonStyle === 'inside',\n            'button-outside': buttonStyle === 'outside',\n            'button-invisible': buttonStyle === 'invisible',\n            'playing': isPlaying,\n        }]\"\n        @click.self=\"togglePlay\"\n    >\n        <canvas\n            class=\"cm-apng-player-canvas\"\n            @click=\"onImageClick\"\n        />\n    </section>\n</template>\n","import { render } from \"./apng-player.vue?vue&type=template&id=010eed6e\"\nimport script from \"./apng-player.vue?vue&type=script&lang=js\"\nexport *