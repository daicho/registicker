 as useStickerUploadImageMutation } from '../../queries/sticker/use-upload-image-mutation';\n\nconst MAX_FILE_SIZE = {\n    sticker: 20 * 1048576,\n    theme: 30 * 1048576,\n    emoji: 20 * 1048576,\n};\nconst MAX_PNG_SIZE = 1048576;\n\nexport default {\n    components: {\n        UploadZipFileForm,\n    },\n    props: {\n        type: {\n            type: String,\n            required: true,\n        },\n        productId: {\n            type: Number,\n            required: true,\n        },\n        keys: {\n            type: Array,\n            required: true,\n        },\n        optionalKeys: {\n            type: Array,\n            default: () => [],\n        },\n        onbeforeupload: {\n            type: Function,\n            default: null,\n        },\n    },\n    emits: ['uploaded'],\n    setup() {\n        const { mutateAsync: uploadStickerImage } =\n            useStickerUploadImageMutation();\n        return { uploadStickerImage };\n    },\n    computed: {\n        maxFileSize() {\n            return MAX_FILE_SIZE[this.type];\n        },\n        pngNotFoundMessage() {\n            const pngNotFoundMessage = {\n                sticker: this.readXlt('upload.error.notfound.from.zip.sticker'),\n                theme: this.readXlt('upload.error.notfound.from.zip.sticker'),\n                emoji: this.readXlt(\n                    'upload.error.notfound.from.zip.emoji.anim.apng',\n                ),\n            };\n            return pngNotFoundMessage[this.type];\n        },\n        fileTooLargeMessage() {\n            const fileTooLargeMessage = {\n                sticker: this.readXlt(\n                    'my.img.validation.filesize.too-large.20mb',\n                ),\n                theme: this.readXlt(\n                    'my.img.validation.filesize.too-large.30mb',\n                ),\n                emoji: this.readXlt(\n                    'my.img.validation.filesize.too-large.20mb',\n                ),\n            };\n            return fileTooLargeMessage[this.type];\n        },\n        pngTooLargeMessage() {\n            return this.readXlt('lp.sticker.less.than.1mb');\n        },\n        uploadImage() {\n            const uploadSticker = ({ key, blob }) => {\n                return this.uploadStickerImage({\n                    stickerId: this.productId,\n                    key: key,\n                    file: blob,\n                });\n            };\n            const uploadTheme = ({ key, blob }) => {\n                return doFetch(\n                    'POST',\n                    `api/${this.type}/${this.productId}/upload_image`,\n                    {\n                        contentType: 'multipart/form-data',\n                        data: {\n                            type: key,\n                            image: blob,\n                        },\n                    },\n                );\n            };\n\n            const uploadEmoji = ({ key, blob }) => {\n                const reader = new FileReader();\n                reader.readAsDataURL(blob);\n                return new Promise(resolve => {\n                    reader.onloadend = function () {\n                        resolve();\n                    };\n                })\n                    .then(() => {\n                        return ImageModel.upload(this.productId, {\n                            content: reader.result,\n                            type: key,\n                        });\n                    })\n                    .then(({ error, url }) => {\n                        return {\n                            errors: error ? [error] : undefined,\n                            success: !error,\n                            url,\n                        };\n                    });\n            };\n\n            const uploaders = {\n                sticker: uploadSticker,\n                theme: uploadTheme,\n                emoji: uploadEmoji,\n            };\n            return uploaders[this.type];\n        },\n    },\n    methods: {\n        onUpload(zipFile) {\n            function createResult(result) {\n                const { imgErrors, uploaded, zipErrors } = result;\n                return {\n                    imgErrors: isEmpty(imgErrors) ? undefined : imgErrors,\n                    uploaded: isEmpty(uploaded) ? undefined : uploaded,\n                    zipErrors: isEmpty(zipErrors) ? undefined : zipErrors,\n                };\n            }\n\n            if (zipFile.size > this.maxFileSize) {\n                this.$emit(\n                    'uploaded',\n                    createResult({\n                        zipErrors: [this.fileTooLargeMessage],\n                    }),\n                );\n                return;\n            }\n            EventBus.$emit('loadingStateChange', true);\n            JSZip.loadAsync(zipFile)\n                .then(filelist => {\n                    return this.filterZipEntries(filelist);\n                })\n                .then(({ entryMap }) => {\n                    if (!this.onbeforeupload) {\n                        return this.uploadImages(entryMap);\n                    }\n                    return Promise.resolve(\n                        this.onbeforeupload({ entryMap }),\n                    ).then(() => this.uploadImages(entryMap));\n                })\n                .then(result => {\n                    this.$emit('uploaded', createResult(result));\n                })\n                .catch(e => {\n                    console.warn(e.message);\n                    let reason;\n                    if (e.message.indexOf('is this a zip file ?') !== -1) {\n                        reason = this.readXlt('upload.error.zipfile.only');\n                    } else {\n                        reason = this.readXlt('upload.error.zipfile.not.open');\n                    }\n\n                    this.$emit(\n                        'uploaded',\n                        createResult({ zipErrors: [reason] }),\n                    );\n                })\n                .finally(e => {\n                    EventBus.$emit('loadingStateChange', false);\n                    if (e && e.message) {\n                        console.warn(e);\n                    }\n                });\n        },\n        filterZipEntries(filelist) {\n            // Filter out valid filenames\n            const entryMap = {};\n            for (const fileKey in filelist.files) {\n                const entry = filelist.files[fileKey];\n                if (fileKey.indexOf('__MACOSX/') > -1 || entry.dir) {\n                    continue;\n                }\n                const filePattern = /^(.+\\/)*(.+)\\.png$/i;\n                const match = entry.name.match(filePattern);\n                if (!match) {\n                    continue;\n                }\n                const imageType = match[2].toLowerCase();\n\n                entryMap[imageType] = entry;\n            }\n            return { entryMap };\n        },\n        uploadImages(entryMap) {\n            const imgErrors = {};\n            const uploaded = {};\n            const zipErrors = [];\n\n            // Extract files and upload separately\n            const fileRequests = [];\n            this.keys.forEach(key => {\n                const entry = entryMap[key];\n                if (!entry) {\n                    if (this.optionalKeys.indexOf(key) >= 0) {\n                        return;\n                    }\n\n                    imgErrors[key] = [\n                        formatString(this.pngNotFoundMessage, { '%1': key }),\n                    ];\n                    return;\n                }\n                const request = entry\n                    .async('blob')\n                    .then(blob => {\n                        if (blob.size > MAX_PNG_SIZE) {\n                            return {\n                                errors: [\n                                    this.readXlt('lp.sticker.less.than.1mb'),\n                                ],\n                            };\n                        }\n                        return this.uploadImage({ key, blob });\n                    })\n                    .then(data => {\n                        if (!data.success) {\n                            imgErrors[key] = data.errors;\n                            return;\n                        }\n                        uploaded[key] = data.url;\n                    })\n                    .catch(errors => {\n                        imgErrors[key] = [errors];\n                    });\n                fileRequests.push(request);\n                delete entryMap[key];\n            });\n\n            Tracking.event({\n                category: 'image',\n                action: 'upload zip',\n                label: 'product',\n                value: this.type,\n            });\n\n            if (Object.keys(entryMap).length > 0) {\n                zipErrors.push(\n                    this.readXlt('upload.error.zipfile.nonconformity'),\n                );\n            }\n\n            return Promise.all(fileRequests).then(() => {\n                return {\n                    imgErrors,\n                    uploaded,\n                    zipErrors,\n                };\n            });\n        },\n    },\n};\n</script>\n<template>\n    <upload-zip-file-form :uploader=\"onUpload\">\n        {{ readXlt('cms.zipfile.upload.button') }}\n        <!-- TODO: The custom-style is here to override rules from parents.\n                Move the <loading-overlay> outside and clean-up the custom style\n                once we finish the whole page's refactoring of image\n                uploading page.\n            -->\n        <loading-overlay custom-style=\"width: 100%; z-index: 1000;\"></loading-overlay>\n    </upload-zip-file-form>\n</template>\n\n","export default {\n    props: {\n        uploader: Function,\n        default: () => {},\n    },\n    template: `\n        <form name=\"uploadZipFileform\">\n            <p class=\"MdBtn01\">\n                <label class=\"mdBtnLabel\">\n                <slot></slot>\n                <input\n                    class=\"mdBtn\"\n                    type=\"file\"\n                    name=\"file\"\n                    @change=\"onUpload\"\n                />\n                </label>\n            </p>\n        </form>\n  `,\n    methods: {\n        onUpload(event) {\n            const target = event.target;\n            const file = target.files[0];\n            this.uploader(file);\n            target.value = null;\n        },\n    },\n};\n","import { render } from \"./zip-extract-uploader.vue?vue&type=template&id=e68a7e1c\"\nimport script from \"./zip-extract-uploader.vue?vue&type=script&lang=js\"\nexport * from \"./zip-extract-uploader.vue?vue&type=script&lang=js\"\n\nimport exportComponent from \"/drone/src/static/cms-next/node_modules/vue-loader/dist/exportHelper.js\"\nconst __exports__ = /*#__PURE__*/exportComponent(script, [['render',render]])\n\nexport default __exports__","<script>\nimport ConfirmDialogController from '../confirm-dialog-controller.vue';\nimport LoadingOverlay from '../base/loading-overlay.vue';\nimport FormTextInput from '../product/form-text-input.vue';\nimport { useAuthorForm, useAuthorErrors } from '../../composables/use-author';\nimport Utils from '../../utils/utils';\n\nexport default {\n    components: {\n        LoadingOverlay,\n        FormTextInput,\n    },\n    extends: ConfirmDialogController,\n    setup() {\n        return {\n            authorErrors: useAuthorErrors(),\n            authorForm: useAuthorForm(),\n        };\n    },\n    data: () => ({\n        selectedLanguage: '',\n        authorModel: null,\n    }),\n    computed: {\n        shouldShowAddButton() {\n            return this.authorModel?.translations.missingLanguages.length > 0;\n        },\n    },\n    beforeMount() {\n        this.authorModel = this.authorForm.data.value.clone();\n        const { translations } = this