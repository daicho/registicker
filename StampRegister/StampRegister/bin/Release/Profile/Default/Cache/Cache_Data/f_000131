.disabledCountries[country.code];\n                      })\n                    : area.countries;\n\n                if (subTotal === 0) {\n                    status[area.code] = 'empty';\n                } else if (subTotal < validCountries.length) {\n                    status[area.code] = 'indeterminate';\n                } else {\n                    status[area.code] = 'checked';\n                }\n            });\n            return status;\n        },\n        checkedCountries() {\n            return this.modelData ? this.modelData : this.internalModelData;\n        },\n    },\n    methods: {\n        onToggleExpand(area) {\n            this.collapsed[area.code] = !this.collapsed[area.code];\n        },\n        onCountryChange(evt, code) {\n            const value = evt.target.checked;\n            this.setModel(code, value);\n            this.$emit('area-code-change', { code, value });\n            this.invokeValidator();\n        },\n        toggleArea(area) {\n            const value = !(this.areaStatus[area.code] === 'checked');\n            const codes = area.countries\n                .map(country => country.code)\n                .filter(code => !this.isCountryDisabled(code));\n\n            codes.forEach(code => this.setModel(code, value));\n            this.$emit('multiple-area-codes-change', { codes, value });\n            this.invokeValidator();\n        },\n        selectAll() {\n            const codes = this.areas\n                .flatMap(area => area.countries.map(country => country.code))\n                .filter(code => !this.isCountryDisabled(code));\n\n            codes.forEach(code => this.setModel(code, true));\n            this.$emit('multiple-area-codes-change', { codes, value: true });\n            this.invokeValidator();\n        },\n        setModel(code, value) {\n            if (!this.modelData) {\n                this.internalModelData[code] = value;\n            }\n        },\n        invokeValidator() {\n            this.va