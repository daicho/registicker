S(this.remainingTime);\n        },\n        remainingTime() {\n            return this.targetTimestamp > this.now\n                ? Math.max(\n                      Math.floor((this.targetTimestamp - this.now) / 1000),\n                      0,\n                  )\n                : 0;\n        },\n    },\n    watch: {\n        interval() {\n            this.resetTarget();\n        },\n    },\n    created() {\n        this.resetTarget();\n    },\n    methods: {\n        resetTarget() {\n            if (this.timer) {\n                this.stopTimer();\n            }\n            if (this.interval <= 0) {\n                return;\n            }\n            this.targetTimestamp = Date.now() + 1000 * this.interval;\n            this.startTimer();\n        },\n        startTimer() {\n            this.timer = setInterval(() => {\n                this.now = Date.now();\n                if (this.remainingTime === 0) {\n                    this.$emit('timeup');\n                    this.stopTimer();\n                }\n            }, 1000);\n        },\n        stopTimer() {\n            if (!this.timer) {\n                return;\n            }\n            clearInterval(this.timer);\n            this.timer = null;\n        },\n    },\n};\n</script>\n<template>\n    <span>{{ remainingInHMS }}</span>\n</template>\n","import { render } from \"./countdown-clock.vue?vue&type=template&id=6f07eda1\"\nimport script from \"./countdown-clock.vue?vue&type=script&lang=js\"\nexport * from \"./countdown-clock.vue?vue&type=script&lang=js\"\n\nimport exportComponent from \"/drone/src/static/cms-next/node_modules/vue-loader/dist/exportHelper.js\"\nconst __exports__ = /*#__PURE__*/exportComponent(script, [['render',render]])\n\nexport default __exports__","import { render } from \"./detail-notice.vue?vue&type=template&id=47422cd7\"\nimport script from \"./detail-notice.vue?vue&type=script&lang=js\"\nexport * from \"./detail-notice.vue?vue&type=script&lang=js\"\n\nimport exportComponent f