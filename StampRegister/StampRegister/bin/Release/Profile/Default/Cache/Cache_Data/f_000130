    hasOaInput() {\n            const { friendshipStatus } = this.dialogs.confirmAgreement;\n            const { countryOrRegion } = this.seller;\n            return shouldShowOACheck(friendshipStatus, countryOrRegion);\n        },\n        isInSpecialEP() {\n            return (\n                this.productState?.campaign?.shopId ===\n                this.SPECIAL_EP_SHOP_ID_2023\n            );\n        },\n    },\n    watch: {\n        productState() {\n            if (this.isInSpecialEP) {\n                this.endpoint\n                    .getSpecialEpCampaign(this.product.id)\n                    .then(result => {\n                        if (!result.specialEpId) {\n                            return;\n                        }\n                        this.hasSpecialEp = true;\n                        this.dialogs.confirmAgreement.additionalCampaign =\n                            result.specialEpId;\n                    })\n                    .catch(() => {\n                        console.log('Network error');\n                    });\n            }\n        },\n    },\n    created() {\n        this.initiateDialog({\n            keys: ['CONFIRM_AGREEMENT', 'REQUEST_LIMIT_EXCEEDED'],\n        });\n    },\n    methods: {\n        ...mapMutations(STORE_DIALOGS, {\n            initiateDialog: 'initiate',\n            hideDialog: 'hide',\n            showDialog: 'show',\n        }),\n        requestReview() {\n            return Utils.showSpinnerUntilDone(\n                this.endpoint.requestReview(this.product.id),\n            );\n        },\n        requestReviewOnUpdateRequest() {\n            return Utils.showSpinnerUntilDone(\n                this.endpoint.requestReviewOnUpdateRequest(this.product.id),\n            );\n        },\n        applyUpdateRequest() {\n            const { id, updateRequestId } = this.product;\n            return Utils.showSpinnerUntilDone(\n                this.endpoint.applyUpdateRequest(id, updateRequestId),\n            );\