Ep\n                ? this.endpoint.updateSpecialEpCampaign\n                : this.endpoint.createSpecialEpCampaign;\n\n            return setSpecialEp(\n                this.product.id,\n                this.productState.campaign.id,\n                this.dialogs.confirmAgreement.additionalCampaign,\n            );\n        },\n        hideDialogOf(key) {\n            return () => this.hideDialog(key);\n        },\n        fetchFriendshipStatus() {\n            this.sharedEndpoint.fetchFriendshipStatus().then(data => {\n                const { confirmAgreement } = this.dialogs;\n                confirmAgreement.friendshipStatus = data.friendship;\n                confirmAgreement.checkedOa =\n                    confirmAgreement.friendshipStatus !== 'BLOCKED';\n            });\n        },\n        sendOaFriendRequest() {\n            this.sharedEndpoint.sendOaFriendRequest().then(data => {\n                if (data.friendship !== 'FRIEND') {\n                    console.warn('Adding OA account failed. Please try again.');\n                }\n            });\n        },\n    },\n};\n</script>\n\n<template>\n    <div\n        v-cloak\n        v-if=\"product.id\"\n        class=\"cm-product-heading\"\n    >\n        <!-- TODO: use true recent_request_count from server side -->\n        <!------ Title and description ------>\n        <div class=\"cm-product-heading-main\">\n            <h1 class=\"cm-product-heading-header\">\n                <span v-if=\"isInfoOnStore\">\n                    {{ readXlt('cms.top.info.publication.store.title') }}\n                </span>\n                <span v-else>\n                    {{ readXlt('cms.top.manege.stickers.title') }}\n                </span>\n            </h1>\n\n            <template v-if=\"!productState.isUpdateRequest\">\n                <p\n                    v-if=\"productState.type === 'sticker' && productState.isExpiredSalePeriod()\"\n                    v-xlt=\"'cms.sales.not.resumed'\"\n                    c