;\n            }\n            return { entryMap };\n        },\n        uploadImages(entryMap) {\n            const imgErrors = {};\n            const uploaded = {};\n            const zipErrors = [];\n\n            // Extract files and upload separately\n            const fileRequests = [];\n            this.keys.forEach(key => {\n                const entry = entryMap[key];\n                if (!entry) {\n                    if (this.optionalKeys.indexOf(key) >= 0) {\n                        return;\n                    }\n\n                    imgErrors[key] = [\n                        formatString(this.pngNotFoundMessage, { '%1': key }),\n                    ];\n                    return;\n                }\n                const request = entry\n                    .async('blob')\n                    .then(blob => {\n                        if (blob.size > MAX_PNG_SIZE) {\n                            return {\n                                errors: [\n                                    this.readXlt('lp.sticker.less.than.1mb'),\n                                ],\n                            };\n                        }\n                        return this.uploadImage({ key, blob });\n                    })\n                    .then(data => {\n                        if (!data.success) {\n                            imgErrors[key] = data.errors;\n                            return;\n                        }\n                        uploaded[key] = data.url;\n                    })\n                    .catch(errors => {\n                        imgErrors[key] = [errors];\n                    });\n                fileRequests.push(request);\n                delete entryMap[key];\n            });\n\n            Tracking.event({\n                category: 'image',\n                action: 'upload zip',\n                label: 'product',\n                value: this.type,\n            });\n\n            if (Object.keys(entryMap).length > 0) {\n                zipErrors.push(\n                    this.readXlt('upload.error.zipfile.nonconformity'),\n                );\n            }\n\n            return Promise.all(fileRequests).then(() => {\n                return {\n                    imgErrors,\n                    uploaded,\n                    zipErrors,\n                };\n            });\n        },\n    },\n};\n</script>\n<template>\n    <upload-zip-file-form :uploader=\"onUpload\">\n        {{ readXlt('cms.zipfile.upload.button') }}\n        <!-- TODO: The custom-style is here to override rules from parents.\n                Move the <loading-overlay> outside and clean-up the custom style\n                once we finish the whole page's refactoring of image\n                uploading page.\n            -->\n        <loading-overlay custom-style=\"width: 100%; z-index: 1000;\"></loading-overlay>\n    </upload-zip-file-form>\n</template>\n\n","export default {\n    props: {\n        uploader: Function,\n        default: () => {},\n    },\n    template: `\n        <form name=\"uploadZipFileform\">\n            <p class=\"MdBtn01\">\n                <label class=\"mdBtnLabel\">\n                <slot></slot>\n                <input\n                    class=\"mdBtn\"\n                    type=\"file\"\n                    name=\"file\"\n                    @change=\"onUpload\"\n                />\n                </label>\n            </p>\n        </form>\n  `,\n    methods: {\n        onUpload(event) {\n            const target = event.target;\n            const file = target.files[0];\n            this.uploader(file);\n            target.value = null;\n        },\n    },\n};\n","import { render } from \"./zip-extract-uploader.vue?vue&type=template&id=e68a7e1c\"\nimport script from \"./zip-extract-uploader.vue?vue&type=script&lang=js\"\nexport * from \"./zip-extract-uploader.vue?vue&type=script&lang=js\"\n\nimport exportComponent from \"/drone/src/static/cms-next/node_modules/vue-loader/dist/exportHelper.js\"\n