 event) => {\n    if (!option) {\n        return { skip: 'accepted' };\n    }\n    if (option instanceof Promise) {\n        return resolveDialogOption(await option);\n    }\n    if (typeof option === 'function') {\n        return resolveDialogOption(option(event));\n    }\n    // Check if the option is a component\n    if ('render' in option) {\n        return { component: option };\n    }\n    return option;\n};\n\nexport const confirmable = {\n    /**\n     * @typedef Binding\n     * @property {String} arg The event type that is going to be listened. e.g. `click`, `change`\n     * @property {DialogParams|Object|Function} value The callback or value for the dialog option\n     *\n     * @param {Element} el\n     * @param {Binding} binding\n     */\n    beforeMount(el, binding) {\n        const { arg: eventType, value } = binding;\n        /**\n         * @param {String} type The event type that is going to be emitted\n         * @param {Object} detail The event detail that will be attached with event\n         */\n        const dispatch = (type, detail) => {\n            const event = new Event(type);\n            event.detail = detail;\n\n            // Always go native events so that it doesn't need to be declared.\n            el.dispatchEvent(event);\n        };\n        const handleEvent = async event => {\n            const option = await resolveDialogOption(value, event);\n            if (option.skip === 'rejected') {\n                return;\n            }\n            if (option.skip === 'accepted') {\n                confirm(null);\n                return;\n            }\n\n            const { component, data, config } = option;\n\n            dialog.open(component, data, config).then(confirm).catch(cancel);\n        };\n\n        const confirm = data => dispatch('confirm', data);\n        const cancel = reason => dispatch('cancel', reason);\n\n        // For now we only allow for listening to native events.\n        // TODO: Use emits to judge if the custom directive should\n        // listen to the Vue custom event or the DOM event of the root element.\n        // see also: https://v3-migration.vuejs.org/breaking-changes/v-on-native-modifier-removed.html\n        el.addEventListener(eventType, handleEvent);\n    },\n};\n","import Areas from '../../models/areas';\n\nexport default {\n    name: 'AreaChecklist',\n    // Area checkboxes: have only computed property `areaStatus`  aggregrated from their child countries' status.\n    // Country checkboxes: have their own status bound on `checkedCountries`.\n    template: `\n    <section>\n        <div class=\"MdCMN28AreaListItem\" v-for=\"area in areas\" :key=\"area.code\">\n            <div class=\"MdCMN27Collapsable\"\n                :class=\"collapsed[area.code] ? 'ExCollapsed' : 'ExExpanded'\"\n                :data-test=\"'collapsable-area-' + area.code\"\n                @click.self=\"onToggleExpand(area)\"\n            >\n                <label>\n                    <input v-if=\"!displayOnly\" type=\"checkbox\"\n                        :checked=\"areaStatus[area.code] === 'checked'\"\n                        :indeterminate=\"areaStatus[area.code] === 'indeterminate'\"\n                        data-test=\"area-checkbox\"\n                        @change=\"toggleArea(area)\"\n                    />\n                    <span>{{ area.name }}</span>\n                    <span v-if=\"displayOnly\"\n                        class=\"MdCMN28AreaListItemLabel\"\n                        v-show=\"areaStatus[area.code] !== 'empty'\"\n                    >\n                        <slot name=\"xltSelectionOfSome\">\n                        </slot>\n                    </span>\n                </label>\n            </div>\n\n            <div class=\"ARSub01\" :hidden=\"collapsed[area.code]\">\n                <template v-if=\"!displayOnly\">\n                    <label\n                        v-for=\"country in area.countries\"\n                        :key=\"country.code\"\