        ],\n                    canEdit,\n                },\n                new Languages(languages),\n            );\n        });\n    }\n\n    constructor(data) {\n        Object.assign(this, data);\n    }\n\n    clone() {\n        const copy = clone(omit(this, ['translations', 'shopTranslations']));\n        copy.translations = this.translations.clone();\n        copy.shopTranslations = this.shopTranslations.clone();\n        return new Author(copy);\n    }\n\n    save() {\n        const url = this.id ? 'api/author/update' : 'api/author/create';\n        return doFetch('POST', url, this.payload).then(({ author }) => {\n            if (author && author.id) {\n                this.id = author.id;\n            }\n            return this;\n        });\n    }\n\n    get payload() {\n        const { id, translations } = this;\n        const meta = translations.listToAuthorMeta();\n        return { data: { id }, dataAsIs: { meta } };\n    }\n\n    get warnings() {\n        if (!this.canEdit) {\n            return {\n                requireReview: false,\n            };\n        }\n        const metaDiff = xorWith(\n            this.translations.list,\n            this.shopTranslations.list,\n            (a, b) => a.name === b.name && a.language.code === b.language.code,\n        );\n        return {\n            requireReview: metaDiff.length > 0,\n        };\n    }\n\n    get errors() {\n        return {\n            required: !this.id,\n        };\n    }\n}\n","export class Deferred {\n    constructor() {\n        this.promise = new Promise((resolve, reject) => {\n            this.resolve = resolve;\n            this.reject = reject;\n        });\n    }\n    resolve() {}\n    reject() {}\n}\n","/**\n * wrap native fetch and expose certain apis\n * @module doFetch\n */\nimport 'whatwg-fetch';\nimport decamelizeKeysDeep from 'decamelize-keys-deep';\nimport camelize from 'camelize';\nimport queryString from 'query-string';\nimport jsonToFormData from 'json-form-data';\nimport pick from 'lodash/pick';\nimport {\n    userStickerCMSXsrfToken,\n    userStickerCMSSellerId,\n} from '../utils/helpers';\n\nconst isAndroid = /Android/i.test(navigator.userAgent);\n\nexport function resolveUrl({ url, params }) {\n    if (/^(?!\\/)/.test(url)) {\n        url = `/my/${userStickerCMSSellerId()}/${url}`;\n    }\n    if (params) {\n        url = `${url}?${queryString.stringify(decamelizeKeysDeep(params))}`;\n    }\n    return url;\n}\n\nexport function resolveContentType({ data, config }) {\n    const { contentType } = config;\n    const isFormDataContent = contentType === 'multipart/form-data';\n    const { hasBlobValue, props } = checkBlobValue(data);\n    if (isFormDataContent || hasBlobValue) {\n        return { contentType: 'multipart/form-data', blobProps: props };\n    }\n\n    if (data) {\n        return { contentType: 'application/json' };\n    }\n\n    // when the request body is empty, don't return the content type\n    return {};\n}\n\nexport function resolveHeaders({ method, contentType, config }) {\n    const isJsonContent = contentType === 'application/json';\n    const prefetchable = method === 'GET' && !isAndroid;\n    const headerItems = prefetchable\n        ? {}\n        : {\n              'X-Requested-With': 'XMLHttpRequest',\n          };\n    const headers = new window.Headers(headerItems);\n    if (/^(post|put|delete)$/i.test(method)) {\n        headers.append('X-XSRF-TOKEN', userStickerCMSXsrfToken());\n    }\n    if (isJsonContent && !prefetchable) {\n        headers.append('Content-Type', 'application/json');\n    }\n    if (config.headers) {\n        Object.keys(config.headers).forEach(key => {\n            headers.append(key, config.headers[key]);\n        });\n    }\n    // browser would decide content-type header for blob itself\n    return headers;\n}\n\nexport function resolveRequestBody({ data, dataAsIs, contentType, blobProps }) {\n    const isJsonContent = contentType === 'application/json';\n    if (!data) {\n        return null;\n    }\n    if (isJsonContent) {\n        return JSON.stringify(\n            Object.assign(decamelizeKeysDeep(data), dataAsIs),\n        );\n    }\n    const formData = new FormData();\n    const jsonToFormDataOptions = {\n        initialFormData: formData,\n        showLeafArrayIndexes: false,\n        includeNullValues: false,\n    };\n    if (blobProps && blobProps.length) {\n        const jsonProps = Object.keys(data).filter(\n            key => !blobProps.includes(key),\n        );\n        const jsonData = pick(data, jsonProps);\n        jsonToFormData(decamelizeKeysDeep(jsonData), jsonToFormDataOptions);\n        blobProps.forEach(key => {\n            const name = decamelizeKeysDeep(key);\n            const value = data[key];\n            if (Array.isArray(value)) {\n                value.forEach(blob => formData.append(`${name}[]`, blob));\n            } else {\n                formData.append(name, value);\n            }\n        });\n    } else {\n        jsonToFormData(decamelizeKeysDeep(data), jsonToFormDataOptions);\n    }\n    if (dataAsIs) {\n        jsonToFormData(dataAsIs, jsonToFormDataOptions);\n    }\n    return formData;\n}\n\nexport function isBlob(val) {\n    return val instanceof Blob;\n}\n\n// for shallow check only\nexport function checkBlobValue(data) {\n    if (!data) {\n        return {\n            hasBlobValue: false,\n            props: [],\n        };\n    }\n    const props = Object.keys(data)\n        .map(prop => {\n            const value = data[prop];\n            if (isBlob(value)) {\n                return prop;\n            }\n            if (Array.isArray(value) && value.length && value.every(isBlob)) {\n                return prop;\n            }\n            return null;\n        })\n        .filter(v => !!v);\n    const hasBlobValue = props.length > 0;\n    return { hasBlobValue, props };\n}\n\nexport function camelizeData(data, ignore) {\n    try {\n        if (Array.isArray(data)) {\n            return data.map(item => {\n                const camelizeProps = Object.keys(item).filter(\n                    key => !ignore.includes(key),\n                );\n                return Object.assign(\n                    camelize(pick(item, camelizeProps)),\n                    pick(item, ignore),\n                );\n            });\n        }\n        const camelizeProps = Object.keys(data).filter(\n            key => !ignore.includes(key),\n        );\n        return Object.assign(\n            camelize(pick(data, camelizeProps)),\n            pick(data, ignore),\n        );\n    } catch (err) {\n        return null;\n    }\n}\n\nexport function convertResponseText(text, config) {\n    if (!text) {\n        return;\n    }\n    // because angular requests to prepend )]}',\n    // to the response of ajax request\n    // so we have to manually remove it\n    if (/^\\)/.test(text)) {\n        text = text.slice(6);\n    }\n    let data;\n    try {\n        data = JSON.parse(text);\n    } catch (error) {\n        return text;\n    }\n    const { camelizeIgnoreAll, camelizeIgnore = [] } = config;\n    return camelizeIgnoreAll ? data : camelizeData(data, camelizeIgnore);\n}\n\n// TODO: some feature can be implemented here\n// 1. should we camelize response here\n// 2. do we need a cache system\n\n/**\n * @typedef Config\n * @type {Object}\n * @property {Object} [params] would be transform to queryString and append to get url\n * @property {Object} [data] content of body\n * @property {Object} [headers] request headers\n * @property {String} [contentType] content type of body\n * @property {Array} [camelizeIgnore] a list containing keys that should not be camelized\n * @property {String} [observe] observe target. Available value in ['response', 'body']\n */\n\n/**\n * @typedef FetchError\n * @type {Object}\n * @property {Int} status code\n * @property {String} text error message\n */\n\n/**\n * return a promise fetch given request settings\n * @template T\n * @return {Promise<T>}\n * @param {String} method http method\n * @param {String} originalUrl url to request\n * @param {Config} config a config instance\n * @throws {Error} an Error and it's messsage is a JSON.stringified {@link FetchError}\n */\nexport function doFetch(method, originalUrl, config = {}) {\n    const { data, dataAsIs, params, observe = 'body' } = config;\n    const url = resolveUrl({ url: originalUrl, params });\n    const { contentType, blobProps } = resolveContentType({ data, config });\n    const headers = resolveHeaders({ method, contentType, config });\n    const body = resolveRequestBody({ data, dataAsIs, contentType, blobProps });\n    const isObservingResponse = observe === 'response';\n    const request = new window.Request(url, {\n        method,\n        headers,\n        // XXX:\n        //   Before Chrome v68, the default credentials will be 'omit'\n        credentials: 'same-origin',\n        body,\n    });\n    return window.fetch(request).then(response => {\n        const { ok, status } = response;\n        if (!ok) {\n            return response.text().then(text => {\n                throw new Error(JSON.stringify({ status, text }));\n            });\n        }\n        if (isObservingResponse) {\n            return response;\n        }\n        return response.text().then(text => convertResponseText(text, config));\n    });\n}\n\nexport default doFetch;\n","import invert from 'lodash/invert';\nimport memoize from 'lodash/memoize';\n\nimport doFetch from './do-fetch';\nimport camelize from 'camelize';\n\nconst fetchLanguages = memoize(() => {\n    return doFetch('GET', 'api/sticker/supported_languages').then(\n        data => data.languages,\n    );\n});\n\nexport class Languages {\n    /**\n     * @typedef Language\n     * @property {String} code\n     * @property {String} name\n     *\n     * @returns {Promise.<Language[]>}\n     */\n    static fetch() {\n        return fetchLanguages();\n    }\n\n    static findLanguageByCode(code, languages) {\n        if (!languages) {\n            throw new Error(`Parameter 'languages' must be given`);\n        }\n        return languages.find(\n            language => camelize(language.code) === camelize(code),\n        );\n    }\n\n    /**\n     * @param {Language[]} languages\n     */\n    constructor(languages) {\n        this.list = languages;\n        languages.forEach(lang => (this[lang.code] = lang));\n    }\n\n    makeSortComparator() {\n        const position = invert(this.list.map(v => v.code));\n        return (a, b) => position[a.code] - position[b.code];\n    }\n}\n","export class ReviewCounter {\n    constructor(count, limit) {\n        this.count = count;\n        this.limit = limit;\n    }\n\n    get isCountZero() {\n        return this.count === 0;\n    }\n\n    get isBelowLimit() {\n        return this.count < this.limit;\n    }\n}\n","import { isOnlySoldInId } from '../utils/component-helpers';\nconst EDITING = 'editing';\nconst WAITING_FOR_REVIEW = 'waiting_for_review';\nconst UPLOADING = 'uploading';\nconst IN_REVIEW = 'in_review';\nconst IN_REVIEW_1 = 'in_review_1';\nconst IN_REVIEW_2 = 'in_review_2';\nconst IN_REVIEW_3 = 'in_review_3';\nconst WAITING_FOR_BILLING = 'waiting_for_billing';\nconst WAITING_FOR_APPROVE = 'waiting_for_approve';\nconst IN_REVIEW_UNDER_PROCESSING = 'in_review_under_processing';\nconst REJECTED = 'rejected';\nconst READY_FOR_SALE = 'ready_for_sale';\nconst APPROVED = 'approved';\nconst STOP_SALE = 'stop_sale';\nconst BANNED = 'banned';\nconst MINIMUM_STICKERS_PER_SET = 8;\n\nexport const STATE_PROPS = {\n    emoji: 'confirmationStatus',\n    sticker: 'confirmationStatusForSeller',\n    theme: 'confirmationStatusForSeller',\n};\n\nconst CAN_RELEASE_PROPS = {\n    sticker: 'canRelease',\n    emoji: 'canReleaseToShop',\n    theme: 'canRelease',\n};\n\nconst DUPLICATED_TITLE_PROPS = {\n    sticker: 'duplicatedTitleError',\n    emoji: 'hasDuplicatedTitle',\n    theme: 'duplicatedTitleError',\n};\n\nconst AREAS_PROPS = {\n   