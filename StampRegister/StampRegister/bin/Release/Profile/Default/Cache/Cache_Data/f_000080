    this.availableLanguages = languages;\n        this.sortComparator = languages.makeSortComparator();\n        this.list = Translatable.sort(translations, this.sortComparator);\n        this.update();\n    }\n\n    static sort(list, comparator) {\n        return list.slice(0).sort((a, b) => comparator(a.language, b.language));\n    }\n\n    add(translation) {\n        this.list.push(translation);\n        this[translation.language.code] = translation;\n        this.update();\n    }\n\n    remove(translation) {\n        const { code } = translation.language;\n        const index = this.list.findIndex(v => v.language.code === code);\n        this.list.splice(index, 1);\n        delete this[code];\n        this.update();\n    }\n\n    update() {\n        this.sorted = Translatable.sort(this.list, this.sortComparator);\n\n        this.missingLanguages = difference(\n            this.availableLanguages.list,\n            this.list.map(v => v.language),\n        );\n    }\n\n    clone() {\n        return new Translatable(cloneDeep(this.list), this.availableLanguages);\n    }\n\n    /**\n     * @typedef {{[code: string]: { name: string }}} AuthorMeta\n     *\n     * @returns {AuthorMeta}\n     */\n    listToAuthorMeta() {\n        return this.list.reduce((x, v) => {\n            return Object.assign(x, { [v.language.code]: { name: v.name } });\n        }, {});\n    }\n}\n","import clone from 'lodash/clone';\nimport isEmpty from 'lodash/isEmpty';\nimport omit from 'lodash/omit';\nimport xorWith from 'lodash/xorWith';\n\nimport { Languages } from './languages';\nimport { Translatable } from './translatable';\nimport doFetch from './do-fetch';\nimport { camelizeData } from './do-fetch';\n\n/**\n * @typedef {{[code: string]: { name: string }}} AuthorMeta\n *\n * @typedef Author\n * @property {string} id\n * @property {AuthorMeta} meta\n * @property {AuthorMeta} shopMeta\n *\n * @typedef AuthorDetail\n * @property {Author[]} authors\n * @property {boolean} canEdit\n *\n * @pa