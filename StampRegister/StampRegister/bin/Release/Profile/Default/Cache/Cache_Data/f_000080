Emoji = ({ key, blob }) => {\n                const reader = new FileReader();\n                reader.readAsDataURL(blob);\n                return new Promise(resolve => {\n                    reader.onloadend = function () {\n                        resolve();\n                    };\n                })\n                    .then(() => {\n                        return ImageModel.upload(this.productId, {\n                            content: reader.result,\n                            type: key,\n                        });\n                    })\n                    .then(({ error, url }) => {\n                        return {\n                            errors: error ? [error] : undefined,\n                            success: !error,\n                            url,\n                        };\n                    });\n            };\n\n            const uploaders = {\n                sticker: uploadSticker,\n                theme: uploadTheme,\n                emoji: uploadEmoji,\n            };\n            return uploaders[this.type];\n        },\n    },\n    methods: {\n        onUpload(zipFile) {\n            function createResult(result) {\n                const { imgErrors, uploaded, zipErrors } = result;\n                return {\n                    imgErrors: isEmpty(imgErrors) ? undefined : imgErrors,\n                    uploaded: isEmpty(uploaded) ? undefined : uploaded,\n                    zipErrors: isEmpty(zipErrors) ? undefined : zipErrors,\n                };\n            }\n\n            if (zipFile.size > this.maxFileSize) {\n                this.$emit(\n                    'uploaded',\n                    createResult({\n                        zipErrors: [this.fileTooLargeMessage],\n                    }),\n                );\n                return;\n            }\n            EventBus.$emit('loadingStateChange', true);\n            JSZip.loadAsync(zipFile)\n                .then(filelist => {\n                    return this.filterZi