        let status = {};\n            this.areas.forEach(area => {\n                let subTotal = 0;\n                area.countries.forEach(country => {\n                    if (this.checkedCountries[country.code]) {\n                        subTotal++;\n                    }\n                });\n\n                let validCountries = this.disabledCountries\n                    ? area.countries.filter(country => {\n                          return !this.disabledCountries[country.code];\n                      })\n                    : area.countries;\n\n                if (subTotal === 0) {\n                    status[area.code] = 'empty';\n                } else if (subTotal < validCountries.length) {\n                    status[area.code] = 'indeterminate';\n                } else {\n                    status[area.code] = 'checked';\n                }\n            });\n            return status;\n        },\n        checkedCountries() {\n            return this.modelData ? this.modelData : this.internalModelData;\n        },\n    },\n    methods: {\n        onToggleExpand(area) {\n            this.collapsed[area.code] = !this.collapsed[area.code];\n        },\n        onCountryChange(evt, code) {\n            const value = evt.target.checked;\n            this.setModel(code, value);\n            this.$emit('area-code-change', { code, value });\n            this.invokeValidator();\n        },\n        toggleArea(area) {\n            const value = !(this.areaStatus[area.code] === 'checked');\n            const codes = area.countries\n                .map(country => country.code)\n                .filter(code => !this.isCountryDisabled(code));\n\n            codes.forEach(code => this.setModel(code, value));\n            this.$emit('multiple-area-codes-change', { codes, value });\n            this.invokeValidator();\n        },\n        selectAll() {\n            const codes = this.areas\n                .flatMap(area => area.countries.map(country => country.code))\n                .filter(code => !this.isCountryDisabled(code));\n\n            codes.forEach(code => this.setModel(code, true));\n            this.$emit('multiple-area-codes-change', { codes, value: true });\n            this.invokeValidator();\n        },\n        setModel(code, value) {\n            if (!this.modelData) {\n                this.internalModelData[code] = value;\n            }\n        },\n        invokeValidator() {\n            this.validator && this.validator();\n        },\n        isCountryDisabled(code) {\n            if (this.disabledCountries && this.disabledCountries[code]) {\n                return true;\n            }\n            return false;\n        },\n    },\n    // This component supports feeding state from parent components from\n    // modelData, which is an object with { [countryCode]: [boolean] } pairs\n    // to indicate each country is checked or not. Undefined countryCodes will\n    // be treated as false when rendering the view.\n    props: {\n        modelData: Object,\n        validator: Function,\n        displayOnly: Boolean,\n        disabledCountries: Object,\n    },\n};\n","export default {\n    inheritAttrs: false,\n    emits: ['tab-change'],\n    template: `\n    <div class=\"cm-product-tablist\">\n        <div class=\"cm-product-tablist-slot\">\n            <div\n                v-for=\"tab in tabs\"\n                :key=\"tab.key\"\n                :class=\"{\n                    selected: currentTab.key === tab.key,\n                    hide: tab.hide\n                }\"\n                :data-test=\"'tab-' + tab.key\"\n                role=\"link\"\n                class=\"cm-product-tablist-tab\"\n                @click=\"onTabChange(tab)\"\n            >\n                <h2 class=\"cm-flexbox cm-flexbox-center\">\n                    <span\n                        v-if=\"tab.prefix\"\n                        v-xlt=\"tab.prefix\"\n                        class=\"cm-flexbox cm-flexbox-center\"\n           