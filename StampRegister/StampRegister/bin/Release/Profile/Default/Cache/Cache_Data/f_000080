appendChild(content);\n            this.setPositionBy(target);\n            this.toggle(true);\n        },\n        onKey: function (evt) {\n            if (evt.key === 'Escape') {\n                this.toggle(false);\n            }\n        },\n        setPositionBy: function (element) {\n            let help = this.$el.querySelector('.mdPOP01Window');\n            let { left, top, width } = element.getBoundingClientRect();\n            const popupLeft =\n                window.pageXOffset + left + width + POPUP_SPACER_WIDTH;\n            const bodyOffsetWidth = document.body.offsetWidth;\n            // put the popup on the left of the icon if it's too wide\n            if (popupLeft + MIN_POPUP_WIDTH <= bodyOffsetWidth) {\n                help.style.right = '';\n                help.style.left = `${popupLeft}px`;\n            } else {\n                help.style.left = '';\n                help.style.right = `${\n                    bodyOffsetWidth -\n                    left -\n                    window.pageXOffset +\n                    POPUP_SPACER_WIDTH\n                }px`;\n            }\n\n            help.style.top = window.pageYOffset + top + 'px';\n        },\n    },\n};\n</script>\n\n<template>\n    <div\n        class=\"MdPOP01Modal FnHelp\"\n        :class=\"{ MdNonDisp: !isShown }\"\n        @click.self=\"toggle()\"\n    >\n        <div class=\"mdPOP01Window ExAbsolute\">\n            <a\n                href=\"\"\n                class=\"mdPOP01Close\"\n                @click.prevent=\"toggle()\"\n            >\n                <span v-xlt=\"'default.close'\" />\n            </a>\n            <div class=\"MdPOP04Mag\" />\n        </div>\n    </div>\n</template>\n","import { render } from \"./help-popup.vue?vue&type=template&id=073171f1\"\nimport script from \"./help-popup.vue?vue&type=script&lang=js\"\nexport * from \"./help-popup.vue?vue&type=script&lang=js\"\n\nimport exportComponent from \"/drone/src/static/cms-next/node_modules/vue-loader/dis